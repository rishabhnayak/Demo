<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.miwok">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Miwok"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CanceledTrains"
            android:label="Canceled Trains"/>
        <activity android:name=".PhrasesActivity"
            android:label="Phrases"/>
        <activity android:name=".TrainRoute"
            android:label="Train Route"/>
        <activity android:name=".DivertedTrains"
            android:label="Diverted Trains"/>
        <activity android:name=".RescheduledTrains"
            android:label="Rescheduled Trains"/>
        <activity android:name=".Select_Train"
            android:label="Select Train"/>
        <activity android:name=".Select_Station"
            android:label="Select Station"/>
        <activity android:name=".Station_Status"
            android:label="Station Status"/>
    </application>

</manifest>